		// "group/system",
		"clock",
		"battery",
		"custom/exit"
	],
	"group/tools": {
		"orientation": "horizontal",
		"modules": []
	},
	"group/system": {
		"orientation": "horizontal",
		"modules": [
			"cpu",
			"memory"
		]
	},
	"custom/notification": {
		"tooltip": false,
		"format": "{icon}",
		"format-icons": {
			"notification": "ÔÇ¢<span foreground='red'><sup></sup></span>",
			"none": "ÔÇ¢",
			"dnd-notification": "Ôá∑",
			"dnd-none": "Ôá∑",
			"inhibited-notification": "ÔÇ¢<span foreground='red'><sup>ÔëÑ</sup></span>",
			"inhibited-none": "ÔÇ¢",
			"dnd-inhibited-notification": "Ôá∑",
			"dnd-inhibited-none": "Ôá∑"
		},
		"return-type": "json",
		"exec-if": "which swaync-client",
		"exec": "swaync-client -swb",
		"on-click": "swaync-client -t -sw",
		"on-click-right": "swaync-client -d -sw",
		"escape": true
	},
	"cpu": {
		"format": "<span font='20' rise='-3pt'>Ó∞ô</span> {usage}%",
		"tooltip": false
	},
	"memory": {
		"interval": 10,
		"format": "<span font='20' rise='0pt'>Û∞æÜ</span> <span rise='3pt'>{used:0.1f}G</span>",
		"format-alt": "<span font='20' rise='0pt'>Û∞æÜ</span> <span rise='3pt'>{percentage}%</span>",
		"format-alt-click": "click",
		"tooltip": true,
		"tooltip-format": "{used:0.1f}GB/{total:0.1f}G",
		"on-click-right": "kitty --title btop sh -c 'btop'"
	},
	"bluetooth": {
		"format": "<span font='20' rise='-1pt'>Ôäì</span>",
		"format-connected-battery": "<span font='18' rise='0pt'>{icon}</span> <span rise='2pt'>{device_battery_percentage}%</span>",
		"format-icons": {
			"default": [
				"Û∞Å∫",
				"Û∞Åª",
				"Û∞Åº",
				"Û∞ÅΩ",
				"Û∞Åæ",
				"Û∞Åø",
				"Û∞ÇÄ",
				"Û∞ÇÅ",
				"Û∞ÇÇ",
				"Û∞Åπ"
			]
		},
		"format-disabled": "",
		"format-off": "",
		"on-click": "~/.config/rofi/applets/bin/bluetooth.sh"
	},
	"custom/exit": {
		"format": "<span font='20' rise='-1pt'>ÔÄë</span>",
		"on-click": "~/.config/rofi/applets/bin/powermenu.sh",
		"tooltip": false
	},
	"hyprland/workspaces": {
		"format": "{icon}",
		"on-click": "activate",
		"show-special": false,
		"tooltip": false,
		"persistent-workspaces": {
			"*": 6
		},
		"format-icons": {
			"1": "Û∞´ç",
			"2": "Û∞´ç",
			"3": "Û∞´ç",
			"4": "Û∞´ç",
			"5": "Û∞´ç",
			"6": "Û∞´ç"
		}
	},
