	"height": 25,
	"width": 1580,
	"modules-left": [
		"custom/notification",
		"tray",
		"bluetooth",
		"pulseaudio",
		"hyprland/language"
	],
	"modules-center": [
		"hyprland/workspaces"
	],
	"modules-right": [
