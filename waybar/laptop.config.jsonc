		"format": "{}",
		"format-en": "EN",
		"format-ru": "RU"
	},
	"tray": {
		"icon-size": 20,
		"spacing": 16
	},
	"clock": {
		"format": "{:L%a, %b %d, %H:%M:%OS} Û∞É∞",
		// "format-alt": "{:L%A, %B %d, %Y (%R)} Û∞É∞",
		"interval": 1,
		"tooltip-format": "{calendar}",
		"locale": "en_GB.UTF-8",
		"calendar": {
			"mode": "month",
			"mode-mon-col": 3,
			"weeks-pos": "",
			"on-scroll": 1,
			"on-click-right": "mode",
			"format": {
				"months": "<span color='#DCD7BA'><b>{}</b></span>",
				"days": "<span color='#727169'><b>{}</b></span>",
				// "weeks":      "<span color='#99ffdd'><b>W{}</b></span>",
				"weekdays": "<span color='#FFB86C'><b>{}</b></span>",
				"today": "<span color='#C34043'><b><u>{}</u></b></span>"
			}
		},
		"actions": {
			"on-click-right": "mode",
			"on-click-forward": "tz_up",
			"on-click-backward": "tz_down",
			"on-scroll-up": "shift_down",
			"on-scroll-down": "shift_up"
		}
	},
	"pulseaudio": {
		"scroll-step": 5,
		"format": "<span font='20' >{icon}</span> <span rise='3pt'>{volume}%</span>",
		"format-icons": {
			"default": [
				"ÔÄ¶",
				"ÔÄß",
				"ÔÄ®"
			]
		},
		// "on-click": "~/.config/rofi/applets/bin/volume.sh"
		"on-click": "pavucontrol"
	},
